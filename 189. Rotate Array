//Soultion 1, copy a new array
class Solution {
    public void rotate(int[] nums, int k) {
        int len = nums.length;
        int[] rotated = new int[len];
        for (int i = 0; i < len; i++) {
            rotated[(k+i) % len] = nums[i];
        }
        for (int i = 0; i < len; i++) {
            nums[i] = rotated[i];
        }
    }
}
